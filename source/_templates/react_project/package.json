{
  "name": "client-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env dom",
    "test.watch": "react-scripts test --env=jsdom --watch",
    "build": "react-scripts build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branch": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
