{
  "elasticsearch": {
    "": "https://www.docker.elastic.co/#",
    "repo": "docker.elastic.co/elasticsearch/elasticsearch",
    "tag": "6.2.3",
    "ports": [
      "9200",
      "9300"
    ],
    "volumes": [
      "/usr/share/elasticsearch/data"
    ],
    "environment": [
      "discovery.type=single-node"
    ],
    "url": "http://elastic:changeme@localhost:9200"
  },

  "nats": {
    "": "https://hub.docker.com/_/nats/",
    "repo": "nats",
    "tag": "latest",
    "ports": [
      "4222",
      "8222"
    ],
    "url": "nats://localhost:4222 / http://localhost:8222"
  },

  "nats-streaming": {
    "": "https://hub.docker.com/_/nats-streaming/",
    "repo": "nats-streaming",
    "tag": "latest",
    "flags": ["-store", "file", "-dir", "/datastore"],
    "ports": [
      "4222",
      "8222"
    ],
    "volumes": [
      "/datastore"
    ],
    "url": "nats://localhost:4222 / http://localhost:8222"
  },

  "mongo": {
    "": "https://hub.docker.com/_/mongo/",
    "repo": "mongo",
    "tag": "latest",
    "flags": "",
    "ports": [
      "27017"
    ],
    "volumes": [
      "/data/db"
    ],
    "url": "mongodb://localhost:27017"
  },

  "postgres": {
    "": "https://hub.docker.com/_/postgres/",
    "repo": "postgres",
    "tag": "alpine",
    "flags": "",
    "ports": [
      "5432"
    ],
    "volumes": [
      "/var/lib/postgresql/data"
    ],
    "environment": [
      "POSTGRES_USER=postgres",
      "POSTGRES_PASSWORD=postgres",
      "POSTGRES_DB=postgres"
    ],
    "url": "postgres://postgres:postgres@localhost:5432/postgres"
  },

  "redis": {
    "": "https://hub.docker.com/_/redis/",
    "repo": "redis",
    "tag": "alpine",
    "flags": "",
    "ports": [
      "6379"
    ],
    "volumes": [
      "/data"
    ],
    "url": "redis://127.0.0.1:6379"
  }
}
